{
  "name": "MCGG",
  "description": "A Web-based minecraft server",
  "keywords": [
    "games",
    "minecraft"
  ],
  "logo": "https://raw.githubusercontent.com/GGJohny/heroku-buildpack-mcgg/master/logo.jpg",
  "repository": "https://github.com/ren232/mcgg",
  "env": {
    "MINECRAFT_EULA": {
      "description": "Do you accept the Minecraft EULA?",
      "value": "true",
      "required": true
    },
    "DROPBOX_API_TOKEN": {
      "description": "Your personal dropbox API token (https://www.dropbox.com/developers/apps Click on create app, Select Dropbox API & App folder, Name the app & create it, Get the API Key.)",
      "required": true
    },
    "DNSEXIT_DNS": {
      "description": "Your personal DnsExit DNS hostname",
      "required": false
    },
    "DNSEXIT_USER": {
      "description": "Your DnsExit Username",
      "required": false
    },
    "DNSEXIT_PASS": {
      "description": "Your DnsExit User password ",
      "required": false
    }
  },
  "buildpacks": [
    {
      "url": "heroku/jvm"
    },
    {
      "url": "https://github.com/ren232/mcgg"
    },
    {
      "url": "https://github.com/heroku/heroku-buildpack-php"
    }
  ]
}
